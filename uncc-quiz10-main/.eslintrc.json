{
    "extends": "eslint:recommended",
    "rules": {
        "no-var": "error",
        "semi": "error",
        "no-multi-spaces": "error",
        "no-multiple-empty-lines": "error",
        "prefer-const": "error",
        "no-use-before-define": "error",
        "quotes": ["error", "double"]
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        },
        "env": {
            "browser": true,
            "node": true
        }
    
    }
}